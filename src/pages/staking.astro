---
import Header from "../components/Header.astro";
import Project from "../components/Project.astro";
import Sidebar from "../components/Sidebar.astro";
---


<!DOCTYPE html>
<html
>
  <Header title="Instant Cryptocurrency Exchange" />
  <body>
   
    <div class="content">
      <Sidebar selectedItem="staking" />
      <nav class="main">
        <div class="container">
          <main class="fold-content-container">
            <div class="fold-content">
              <h1><br />Staking Cryptocurrency</h1>
              <p class="lead">
               Fast Crypto Exchange The Average Time for Exchange is About 2 minutes. You Can Track The Crypto Swap Progress on The Exchange Page.
              </p>
            </div>
            <script defer src="https://www.livecoinwatch.com/static/lcw-widget.js"></script> <div class="livecoinwatch-widget-5" lcw-base="USD" lcw-color-tx="#0693e3" lcw-marquee-1="coins" lcw-marquee-2="coins" lcw-marquee-items="30" ></div>

     <!-- TradingView Widget BEGIN -->
<div class="tradingview-widget-container">
  <div class="tradingview-widget-container__widget"></div>
  <div class="tradingview-widget-copyright"><a href="https://www.tradingview.com/" rel="noopener nofollow" target="_blank"><span class="blue-text"></span></a></div>
  <script type="text/javascript" src="https://s3.tradingview.com/external-embedding/embed-widget-symbol-overview.js" async>
  {
  "lineWidth": 2,
  "lineType": 0,
  "chartType": "area",
  "fontColor": "rgb(106, 109, 120)",
  "gridLineColor": "rgba(242, 242, 242, 0.06)",
  "volumeUpColor": "rgba(34, 171, 148, 0.5)",
  "volumeDownColor": "rgba(247, 82, 95, 0.5)",
  "backgroundColor": "#0F0F0F",
  "widgetFontColor": "#DBDBDB",
  "upColor": "#22ab94",
  "downColor": "#f7525f",
  "borderUpColor": "#22ab94",
  "borderDownColor": "#f7525f",
  "wickUpColor": "#22ab94",
  "wickDownColor": "#f7525f",
  "colorTheme": "dark",
  "isTransparent": false,
  "locale": "en",
  "chartOnly": false,
  "scalePosition": "right",
  "scaleMode": "Normal",
  "fontFamily": "-apple-system, BlinkMacSystemFont, Trebuchet MS, Roboto, Ubuntu, sans-serif",
  "valuesTracking": "1",
  "changeMode": "price-and-percent",
  "symbols": [
    [
      "MARKETSCOM:ETHEREUM|1D"
    ]
  ],
  "dateRanges": [
    "1d|1",
    "1m|30",
    "3m|60",
    "12m|1D",
    "60m|1W",
    "all|1M"
  ],
  "fontSize": "10",
  "headerFontSize": "medium",
  "autosize": true,
  "width": "100%",
  "height": "100%",
  "noTimeScale": false,
  "hideDateRanges": false,
  "hideMarketStatus": false,
  "hideSymbolLogo": false
}
  </script>
</div>
<!-- TradingView Widget END -->
          </main>
          <div class="projects-section">
            
             <!-- Software Development -->
            <a href="https://reyhub.com" class="projects-container">
              <div class="single-projects-category">
                <div class="projects-category-image-container">
                  
             <img src="./assets/eth.svg" loading="lazy" alt="" class="projects-category-image" />  

                </div>
              </div>
              <div class="single-projects-content">
                <h4>Ethereum Ecosystem</h4>
                <div class="tech-tools-container">
                  <div class="mini-label-white gray-color">Pick One Of 1 M+ Supported Currency Pair From The List.</div>
                  <div class="chips-container chips-container-small-width">
                    <div class="single-chip small-chip">Bitcoin</div>
                    <div class="single-chip small-chip">Ethereum</div>
                    <div class="single-chip small-chip">Solana</div>
                    <div class="single-chip small-chip">Binance</div>
                    <div class="single-chip small-chip">Avalanche</div>
                    <div class="single-chip small-chip">Base</div>
                    <div class="single-chip small-chip">Atom</div>
                    <div class="single-chip small-chip">Polygon</div>
                    <div class="single-chip small-chip">Pulse</div>
                    <div class="single-chip small-chip">Ripple</div>
                    <div class="single-chip small-chip">Doge</div>
                    <div class="single-chip small-chip">Tron</div>
                    <div class="single-chip small-chip">Cardano</div>
                    
                  </div>
                </div>
              </div>
            </a>
            
          
            <!-- DevOps & Cloud -->
            <a href="/devops" class="projects-container">
              <div class="single-projects-category">
                <div class="projects-category-image-container devops-img">

                 <img src="./assets/eth.svg" loading="lazy" alt="" class="projects-category-image" />

                </div>
              </div>
              <div class="single-projects-content">
                <h4>Receive Crypto</h4>
                <div class="tech-tools-container">
                  <div class="mini-label-white gray-color">Receive Exchanged Crypto With The Best Possible Rate.</div>
                  <div class="chips-container chips-container-small-width">
                    <div class="single-chip small-chip">Terraform</div>
                    <div class="single-chip small-chip">Azure</div>
                    <div class="single-chip small-chip">AWS</div>
                    <div class="single-chip small-chip">Docker</div>
                    <div class="single-chip small-chip">Kubernetes</div>
                    <div class="single-chip small-chip">GitLab CI/CD</div>
                    <div class="single-chip small-chip">RKE</div>
                    <div class="single-chip small-chip">Node.js</div>
                    <div class="single-chip small-chip">RabbitMQ</div>
                    <div class="single-chip small-chip">Linux</div>
                    <div class="single-chip small-chip">Bash</div>
                    <div class="single-chip small-chip">Grafana</div>
                    <div class="single-chip small-chip">Prometheus</div>
                  </div>
                </div>
              </div>
            </a>
          </div>
        </div>
      </nav>
    </div>

